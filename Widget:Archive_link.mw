<!--{* For Documentation see Template:Archive_link . Weird formatting due to preventing spaces (Css important) *}-->

<!--{if ! empty( $onion ) && $onion != 'none'}--><!--{$isOnion=true}--><!--{else}--><!--{$isOnion=false}--><!--{/if}-->

<span class="archive-link<!--{if isset( $linewrap ) && $linewrap == 'true'}--> line-wrap<!--{/if}-->">
 <a
  href="<!--{$url|escape:'html'}-->"
  class="external"
  rel="noreferrer noopener<!--{if $isOnion && $onion == 'copy'}--> nofollow<!--{/if}-->"
 ><!--{$text}--></a><!--{if isset( $archive ) && $archive != 'none'}--><sup class="ext-link-to-archive<!--{if $isOnion}--> has-sibling<!--{/if}-->" title="Link to archived version">
  <a
   target="_blank"
   rel="noreferrer noopener"
   title="Link to archived version"
   href="<!--{if isset( $archive ) && $archive == 'copy'}--><!--{$url|escape:'html'}--><!--{else}-->https://web.archive.org/web/<!--{$url|escape:'html'}--><!--{/if}-->" >
   <img src="/w/images/7/73/Internet_Archive_logo.png" width="14" height="16" alt="The Web Archive" />
  </a>
 </sup><!--{/if}--><!--{if $isOnion}--><sup class="ext-link-to-archive" title="Link to alternative onion url">
  <a
   target="_blank"
   rel="noreferrer noopener<!--{if $isOnion}--> nofollow<!--{/if}-->"
   title="Link to alternative onion url"
   href="<!--{if $isOnion && $onion == 'copy'}--><!--{$url|escape:'html'}--><!--{else}--><!--{$onion|escape:'html'}--><!--{/if}-->" >
   <img src="/w/images/thumb/a/a8/Iconfinder_tor_386502.png/40px-Iconfinder_tor_386502.png" width="16" height="16" alt="Onion Version" />
  </a>
 </sup>
</span>
<!--{/if}--><noinclude>
