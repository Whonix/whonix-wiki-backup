/* Roboto (standard font) */

@font-face {
	font-family: 'Roboto';
	src: url('/libs/Roboto/Roboto-Regular-webfont.woff');
	font-display: swap;
}

@font-face {
	font-family: 'Roboto';
	font-weight: 300;
	src: url('/libs/Roboto/Roboto-Light-webfont.woff');
	font-display: swap;
}

/* Cousine (monospace font when needed) */

@font-face {
	font-family: 'Cousine';
	src: url('/libs/Cousine/Cousine-Regular.woff');
	font-display: swap;
}